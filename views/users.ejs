<div class="users-container">
  <h1>Trusted Realtors</h1>
  <% users.forEach(user => { %>
    <div class="card users-card">
      <div class="card-content">
      <h2><%=user.name%></h2>
      <h4><a href="mailto: <%=user.email%>"><%= user.email %></a></h4>
      <p>Realtor since <%=(user.createdAt).toDateString()%></p>
      <a href="/user/<%=user._id%>">Check out listings</a>
    </div>
    </div>
<%}) %>
</div>
